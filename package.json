{
  "name": "random",
  "version": "2.2.0",
  "description": "Seedable random number generator supporting many common distributions.",
  "main": "dist/random.js",
  "module": "dist/random.js",
  "types": "dist/random.d.ts",
  "repository": "transitive-bullshit/random",
  "author": "Travis Fischer <travis@automagical.ai>",
  "license": "MIT",
  "reveal": true,
  "scripts": {
    "start": "yarn build:watch",
    "docs": "update-markdown-jsdoc -f dist/random.js --shallow",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prebuild": "yarn run clean",
    "clean": "del dist",
    "prepublishOnly": "yarn run build",
    "test": "ava -v",
    "seed-test": "ava --match='*seed*'",
    "lint": "eslint '{src,test}/**/*.ts' --fix"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "random",
    "number",
    "generator",
    "rng",
    "prng",
    "stats",
    "d3-random",
    "seedrandom",
    "distribution",
    "pseudorandom",
    "uniform",
    "normal",
    "gaussian",
    "lognormal",
    "poisson",
    "exponential",
    "irwinhall",
    "bates"
  ],
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/node": "^14.14.31",
    "@types/seedrandom": "^2.4.28",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "ava": "^3.15.0",
    "del-cli": "^3.0.1",
    "dev-cli": "^0.0.0",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "update-markdown-jsdoc": "^1.0.11"
  },
  "dependencies": {
    "ow": "^0.23.0",
    "seedrandom": "^3.0.5"
  },
  "files": [
    "dist"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
