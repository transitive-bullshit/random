{
  "name": "random",
  "version": "2.0.6",
  "description": "Seedable random number generator supporting many common distributions.",
  "main": "index.js",
  "repository": "transitive-bullshit/random",
  "author": "Travis Fischer <travis@automagical.ai>",
  "license": "MIT",
  "scripts": {
    "docs": "update-markdown-jsdoc -f src/random.js --shallow",
    "build": "babel -s -d dist src",
    "build:watch": "babel -w -s -d dist src",
    "prebuild": "yarn run clean",
    "clean": "del dist",
    "prepublishOnly": "yarn run build",
    "test": "ava -v && standard"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "random",
    "number",
    "generator",
    "rng",
    "prng",
    "stats",
    "d3-random",
    "seedrandom",
    "distribution",
    "pseudorandom",
    "uniform",
    "normal",
    "gaussian",
    "lognormal",
    "poisson",
    "exponential",
    "irwinhall",
    "bates"
  ],
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "del-cli": "^1.1.0",
    "esm": "^3.0.48",
    "seedrandom": "^2.4.3",
    "standard": "^11.0.0",
    "update-markdown-jsdoc": "^1.0.2"
  },
  "dependencies": {
    "ow": "^0.4.0"
  },
  "files": [
    "index.js",
    "dist"
  ],
  "ava": {
    "require": [
      "esm"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
